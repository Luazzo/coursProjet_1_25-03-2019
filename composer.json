{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.9",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.6",
        "easycorp/easyadmin-bundle": "^2.1",
        "friendsofsymfony/user-bundle": "~2.1.2",
        "jms/serializer": "^1.14",
        "knplabs/knp-paginator-bundle": "^3.0",
        "sensio/framework-extra-bundle": "^5.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/apache-pack": "1.0.1",
        "symfony/asset": "4.2.*",
        "symfony/console": "4.2.*",
        "symfony/dotenv": "4.2.*",
        "symfony/expression-language": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.2.*",
        "symfony/framework-bundle": "4.2.*",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "1.0.6",
        "symfony/process": "4.2.5",
        "symfony/security-bundle": "4.2.*",
        "symfony/serializer-pack": "1.0.2",
        "symfony/swiftmailer-bundle": "^3.2.5",
        "symfony/translation": "4.2.*",
        "symfony/twig-bundle": "4.2.*",
        "symfony/validator": "4.2.*",
        "symfony/web-link": "4.2.*",
        "symfony/webpack-encore-bundle": "^1.5",
        "symfony/yaml": "4.2.*",
        "vich/uploader-bundle": "^1.8"
    },
    "require-dev": {
        "symfony/debug-pack": "1.0.7",
        "symfony/dotenv": "4.2.5",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "1.0.4",
        "symfony/test-pack": "^1.0",
        "symfony/web-server-bundle": "4.2.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
